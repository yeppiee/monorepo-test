{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "CompanionPlanting": {
            "description": "Companion planting relationships - which plants help or hurt each other",
            "properties": {
                "allelopathicEffects": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "badCompanions": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "goodCompanions": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "type": "object"
        },
        "CompetitiveNature": {
            "description": "How aggressively plants compete for resources - affects spacing",
            "enum": [
                "aggressive",
                "moderate",
                "gentle"
            ],
            "type": "string"
        },
        "Distance": {
            "properties": {
                "unit": {
                    "$ref": "#/definitions/RawDistanceUnit"
                },
                "value": {
                    "type": "number"
                }
            },
            "required": [
                "unit",
                "value"
            ],
            "type": "object"
        },
        "DistanceRange": {
            "properties": {
                "max": {
                    "$ref": "#/definitions/Distance"
                },
                "min": {
                    "$ref": "#/definitions/Distance"
                }
            },
            "required": [
                "max",
                "min"
            ],
            "type": "object"
        },
        "DrainagePreference": {
            "description": "How quickly water moves through soil - affects plant health and watering",
            "enum": [
                "well-draining",
                "moisture-retentive",
                "wet",
                "dry"
            ],
            "type": "string"
        },
        "EdiblePart": {
            "description": "Which parts of the plant are edible",
            "enum": [
                "leaves",
                "fruit",
                "root",
                "flower",
                "stem",
                "seed"
            ],
            "type": "string"
        },
        "EnvironmentalRequirements": {
            "description": "Environmental conditions the plant needs to thrive",
            "properties": {
                "hardiness": {
                    "properties": {
                        "coolSeasonHardy": {
                            "type": "boolean"
                        },
                        "frostHardy": {
                            "type": "boolean"
                        },
                        "heatTolerant": {
                            "type": "boolean"
                        },
                        "usdaZone": {
                            "$ref": "#/definitions/USDAHardinessZoneRangeMap"
                        }
                    },
                    "required": [
                        "usdaZone"
                    ],
                    "type": "object"
                },
                "light": {
                    "$ref": "#/definitions/LightPreference"
                },
                "soil": {
                    "properties": {
                        "drainage": {
                            "$ref": "#/definitions/DrainagePreference"
                        },
                        "organicMatter": {
                            "$ref": "#/definitions/OrganicMatterPreference"
                        },
                        "ph": {
                            "$ref": "#/definitions/PhPreference"
                        },
                        "texture": {
                            "$ref": "#/definitions/SoilTexture"
                        }
                    },
                    "type": "object"
                },
                "temperatureRanges": {
                    "properties": {
                        "ideal": {
                            "$ref": "#/definitions/TemperatureRange"
                        },
                        "soilMin": {
                            "$ref": "#/definitions/Temperature"
                        },
                        "transplant": {
                            "$ref": "#/definitions/TemperatureRange"
                        }
                    },
                    "type": "object"
                },
                "water": {
                    "$ref": "#/definitions/WaterNeeds"
                }
            },
            "required": [
                "light",
                "water"
            ],
            "type": "object"
        },
        "GrowthCharacteristics": {
            "description": "Physical growth characteristics that affect garden design",
            "properties": {
                "habit": {
                    "$ref": "#/definitions/PlantGrowthHabit"
                },
                "lifespan": {
                    "$ref": "#/definitions/PlantLifespan"
                },
                "matureSize": {
                    "properties": {
                        "height": {
                            "$ref": "#/definitions/DistanceRange"
                        },
                        "spread": {
                            "$ref": "#/definitions/DistanceRange"
                        }
                    },
                    "required": [
                        "height",
                        "spread"
                    ],
                    "type": "object"
                },
                "rate": {
                    "$ref": "#/definitions/PlantGrowthRate"
                },
                "rootDepth": {
                    "$ref": "#/definitions/RootDepth"
                },
                "supportNeeds": {
                    "$ref": "#/definitions/SupportNeeds"
                }
            },
            "required": [
                "habit",
                "lifespan",
                "matureSize",
                "rootDepth",
                "supportNeeds"
            ],
            "type": "object"
        },
        "HarvestMethod": {
            "description": "How plants are harvested affects garden planning and succession planting",
            "enum": [
                "cut-and-come-again",
                "single-harvest",
                "continuous",
                "periodic"
            ],
            "type": "string"
        },
        "LightPreference": {
            "description": "Light requirements - critical for photosynthesis and plant health",
            "enum": [
                "full-sun",
                "partial-sun",
                "shade"
            ],
            "type": "string"
        },
        "NutrientNeeds": {
            "description": "Nutrient requirements affect fertilization and companion planting",
            "enum": [
                "light",
                "moderate",
                "heavy"
            ],
            "type": "string"
        },
        "OrganicMatterPreference": {
            "description": "Organic matter content affects soil fertility and structure",
            "enum": [
                "low",
                "moderate",
                "high",
                "very-high"
            ],
            "type": "string"
        },
        "PhPreference": {
            "description": "Soil acidity/alkalinity affects nutrient availability to plants\npH scale: 0-14, where 7 is neutral, <7 is acidic, >7 is alkaline",
            "enum": [
                "acidic",
                "neutral",
                "alkaline",
                "adaptable"
            ],
            "type": "string"
        },
        "PlantCareInfo": {
            "description": "Plant care and maintenance information",
            "properties": {
                "commonDiseases": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "commonPests": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "organicTreatments": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "vulnerabilityPeriods": {
                    "items": {
                        "$ref": "#/definitions/PlantingSeason"
                    },
                    "type": "array"
                }
            },
            "type": "object"
        },
        "PlantFamily": {
            "description": "Plant family names used for companion planting relationships.\nThese represent common groupings that gardeners use when planning\nwhich plants grow well together or should be kept apart.",
            "enum": [
                "buttercups",
                "irises",
                "begonias",
                "arums",
                "cannas",
                "ranunculuses",
                "daisies",
                "peonies",
                "snapdragons",
                "geraniums",
                "grasses",
                "tomatoes",
                "peppers",
                "beans",
