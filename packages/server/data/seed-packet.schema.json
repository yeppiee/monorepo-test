                "pumpkins",
                "cucumbers",
                "carrots",
                "onions",
                "garlic",
                "shallots",
                "leeks",
                "lettuce",
                "spinach",
                "radishes",
                "beets",
                "potatoes",
                "sweet potatoes",
                "turnips",
                "rutabagas",
                "brassicas",
                "cabbage",
                "broccoli",
                "cauliflower",
                "kale",
                "kohlrabi",
                "mustard",
                "arugula",
                "nightshades",
                "eggplant",
                "cucurbits",
                "watermelons",
                "cantaloupes",
                "alliums",
                "legumes",
                "basil",
                "parsley",
                "chives",
                "dill",
                "sage",
                "oregano",
                "rosemary",
                "thyme",
                "cilantro",
                "mint",
                "lavender",
                "aromatic herbs",
                "marigolds",
                "nasturtiums",
                "sunflowers",
                "roses",
                "calendula",
                "cosmos",
                "zinnias",
                "grains",
                "wheat",
                "oats",
                "apples",
                "cherries",
                "apricots",
                "figs",
                "berries",
                "strawberries",
                "blueberries",
                "raspberries",
                "blackberries",
                "fennel",
                "asparagus",
                "parsnips",
                "melons",
                "walnut trees",
                "pole beans",
                "celery",
                "okra",
                "artichokes",
                "rhubarb",
                "clover",
                "buckwheat",
                "none known"
            ],
