        "PlantFunction": {
            "description": "Primary purpose/function of plants in the garden ecosystem",
            "enum": [
                "food",
                "pollinator",
                "pest-repellent",
                "nitrogen-fixer",
                "ground-cover",
                "windbreak",
                "ornamental"
            ],
            "type": "string"
        },
        "PlantFunctions": {
            "description": "What role the plant plays in the garden ecosystem",
            "properties": {
                "beneficialInsects": {
                    "type": "boolean"
                },
                "pollinatorValue": {
                    "type": "boolean"
                },
                "primary": {
                    "$ref": "#/definitions/PlantFunction"
                },
                "secondary": {
                    "items": {
                        "$ref": "#/definitions/PlantFunction"
                    },
                    "type": "array"
                }
            },
            "required": [
                "primary"
            ],
            "type": "object"
        },
        "PlantGrowthHabit": {
            "description": "Physical growth patterns that affect garden layout and support needs",
            "enum": [
                "bush",
                "vine",
                "tree",
                "groundcover",
                "upright",
                "rosette",
                "climbing",
                "trailing"
            ],
            "type": "string"
        },
        "PlantGrowthRate": {
            "description": "How quickly plants reach maturity - affects garden planning timelines",
            "enum": [
                "slow",
                "moderate",
                "fast"
            ],
            "type": "string"
        },
        "PlantLifespan": {
            "description": "Plant lifespan determines how long a plant lives and when it reproduces",
            "enum": [
                "annual",
                "biennial",
                "perennial"
            ],
            "type": "string"
        },
        "PlantPart": {
            "description": "Which parts of the plant may be toxic (important for plants with mixed safety)",
            "enum": [
                "whole-plant",
                "leaves",
                "stems",
                "roots",
                "flowers",
                "fruits",
                "seeds",
                "sap",
                "bark",
                "green-parts"
            ],
            "type": "string"
        },
        "PlantPropagationMethod": {
            "description": "How plants are typically reproduced/started",
            "enum": [
                "seed",
                "vegetative",
                "grafted"
            ],
            "type": "string"
        },
        "PlantSpacing": {
            "description": "All spacing requirements for optimal plant growth and garden layout",
            "properties": {
                "canIntercrop": {
                    "type": "boolean"
                },
                "minimum": {
                    "$ref": "#/definitions/Distance"
                },
                "optimal": {
                    "$ref": "#/definitions/Distance"
                },
                "rowSpacing": {
                    "$ref": "#/definitions/Distance"
                },
                "thinning": {
                    "$ref": "#/definitions/DistanceRange"
                }
            },
            "required": [
                "minimum",
                "optimal"
            ],
            "type": "object"
        },
        "PlantStorageStructure": {
            "description": "Storage structures are specialized plant parts that store energy/nutrients.\nThese affect how plants are propagated and planted.",
            "enum": [
                "bulb",
                "tuber",
                "rhizome",
                "corm",
                "crown",
                "roots"
            ],
            "type": "string"
        },
        "PlantingInfo": {
            "description": "When and how to plant",
            "properties": {
                "daysToGermination": {
                    "$ref": "#/definitions/TimeDurationRange"
                },
                "germinationDuration": {
                    "$ref": "#/definitions/TimeDuration"
                },
                "method": {
                    "$ref": "#/definitions/PlantingMethod"
                },
                "seedDepth": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Distance"
                        },
                        {
                            "$ref": "#/definitions/DistanceRange"
                        }
                    ]
                },
                "timing": {
                    "properties": {
                        "seasons": {
                            "items": {
                                "$ref": "#/definitions/PlantingSeason"
                            },
                            "type": "array"
                        },
                        "weeksAfterLastFrost": {
                            "type": "number"
                        },
                        "weeksBeforeLastFrost": {
                            "type": "number"
                        }
                    },
                    "type": "object"
                }
            },
            "required": [
                "method"
            ],
            "type": "object"
        },
        "PlantingMethod": {
            "description": "How plants are started in the garden",
            "enum": [
                "direct-sow",
                "transplant",
                "both"
            ],
            "type": "string"
        },
        "PlantingSeason": {
            "description": "Planting seasons based on temperature and frost patterns",
            "enum": [
                "early-spring",
                "spring",
                "late-spring",
                "summer",
                "fall",
                "winter"
            ],
            "type": "string"
        },
        "ProductionInfo": {
            "description": "Information about harvesting and food production (only for edible plants)",
            "properties": {
                "edibleParts": {
                    "items": {
                        "$ref": "#/definitions/EdiblePart"
                    },
                    "type": "array"
                },
                "harvestDuration": {
                    "$ref": "#/definitions/TimeDuration"
                },
                "harvestMethod": {
                    "$ref": "#/definitions/HarvestMethod"
                },
                "harvestWindow": {
                    "$ref": "#/definitions/TimeDuration"
                },
                "producesFruit": {
                    "type": "boolean"
                },
                "successionPlanting": {
                    "properties": {
                        "intervalWeeks": {
                            "type": "number"
                        },
                        "maxSuccessions": {
                            "type": "number"
                        },
                        "recommended": {
                            "type": "boolean"
                        },
                        "seasonalLimitations": {
                            "items": {
                                "$ref": "#/definitions/PlantingSeason"
                            },
                            "type": "array"
                        }
                    },
                    "type": "object"
                }
            },
            "required": [
                "producesFruit"
            ],
            "type": "object"
        },
        "RGBColor": {
            "properties": {
                "blue": {
                    "type": "number"
                },
                "green": {
                    "type": "number"
                },
                "red": {
                    "type": "number"
                }
            },
            "required": [
                "blue",
                "green",
                "red"
            ],
            "type": "object"
        },
        "RawDistanceUnit": {
            "enum": [
                "centimeters",
                "feet",
                "inches",
                "meters",
                "yards"
            ],
            "type": "string"
        },
        "RawSeedPacketInfo": {
            "description": "Complete information about a plant variety, typically from a seed packet\nor plant tag. This model supports garden planning and plant placement\noptimization by capturing all relevant growing characteristics.\n\nThe structure is designed to handle different plant types:\n- Food crops (vegetables, fruits, herbs)\n- Ornamental plants (flowers, decorative plants)\n- Functional plants (nitrogen fixers, pest repellents)\n- Trees and perennials\n\nNot all fields will be available for every plant - optional sections\nallow for incomplete data while still being useful for garden planning.",
            "properties": {
                "care": {
                    "$ref": "#/definitions/PlantCareInfo"
                },
                "commonName": {
                    "type": "string"
                },
                "companions": {
                    "$ref": "#/definitions/CompanionPlanting"
                },
                "description": {
                    "type": "string"
                },
                "environmental": {
                    "$ref": "#/definitions/EnvironmentalRequirements"
                },
                "functions": {
                    "$ref": "#/definitions/PlantFunctions"
                },
                "growth": {
                    "$ref": "#/definitions/GrowthCharacteristics"
                },
                "id": {
                    "type": "string"
                },
                "latinName": {
                    "type": "string"
                },
                "plantFamily": {
                    "$ref": "#/definitions/PlantFamily"
                },
                "planting": {
                    "$ref": "#/definitions/PlantingInfo"
                },
                "plantingInstructions": {
                    "type": "string"
                },
                "presentation": {
                    "$ref": "#/definitions/SeedPacketPresentation"
                },
                "production": {
                    "$ref": "#/definitions/ProductionInfo"
                },
                "propagationMethod": {
                    "$ref": "#/definitions/PlantPropagationMethod"
                },
                "resources": {
                    "$ref": "#/definitions/ResourceRequirements"
                },
                "seedPacketInfo": {
                    "$ref": "#/definitions/SeedPacketInfo"
                },
                "seedSource": {
                    "type": "string"
                },
                "sellBy": {
                    "description": "Plant and Seed Packet Data Model\n\nThis file defines the complete data structure for representing plants, seeds, and their\ncharacteristics for garden planning and plant placement optimization.\n\nKey Concepts for Non-Gardeners:\n- Annual: Lives one growing season (e.g., tomatoes, lettuce)\n- Perennial: Lives multiple years (e.g., fruit trees, asparagus)\n- Companion Planting: Some plants grow better together, others inhibit each other\n- Succession Planting: Planting the same crop multiple times for continuous harvest\n- Hardiness Zone: Geographic regions based on average minimum winter temperatures",
                    "type": "string"
                },
                "spacing": {
                    "$ref": "#/definitions/PlantSpacing"
                },
                "storageStructure": {
                    "$ref": "#/definitions/PlantStorageStructure"
                },
                "toxInfo": {
                    "$ref": "#/definitions/ToxicityInfo"
                }
            },
            "required": [
                "commonName",
                "environmental",
                "functions",
                "growth",
                "id",
                "plantFamily",
                "planting",
                "presentation",
                "propagationMethod",
                "resources",
                "spacing"
            ],
            "type": "object"
        },
        "ResourceRequirements": {
            "description": "Resource consumption patterns for garden planning",
            "properties": {
                "competitiveNature": {
                    "$ref": "#/definitions/CompetitiveNature"
                },
                "nutrients": {
                    "$ref": "#/definitions/NutrientNeeds"
                }
            },
            "required": [
                "competitiveNature",
                "nutrients"
            ],
            "type": "object"
        },
        "RootDepth": {
            "description": "Root depth affects water/nutrient competition and companion planting",
            "enum": [
                "shallow",
                "medium",
                "deep"
            ],
            "type": "string"
        },
        "SeedPacketInfo": {
            "description": "Information specific to seed packets (not live plants)",
            "properties": {
                "germinationRate": {
                    "type": "number"
                },
                "ntWeightInOz": {
                    "type": "number"
                },
                "seedCount": {
                    "type": "number"
                },
                "storageRequirements": {
                    "type": "string"
                },
                "treatmentRequired": {
                    "items": {
                        "$ref": "#/definitions/SeedTreatment"
                    },
                    "type": "array"
                },
                "viabilityYears": {
                    "type": "number"
                }
            },
            "type": "object"
        },
        "SeedPacketPresentation": {
            "properties": {
                "accentColor": {
                    "$ref": "#/definitions/RGBColor"
                },
                "iconPath": {
                    "type": "string"
                }
            },
            "required": [
                "accentColor",
                "iconPath"
            ],
            "type": "object"
        },
        "SeedTreatment": {
            "description": "Special treatments some seeds need before planting to germinate properly",
            "enum": [
                "scarification",
                "stratification",
                "soaking"
            ],
            "type": "string"
        },
        "SoilTexture": {
            "description": "Soil texture affects drainage, nutrient retention, and root penetration",
            "enum": [
                "sandy",
                "silt",
                "clay",
                "loamy",
                "rocky"
