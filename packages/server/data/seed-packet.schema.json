                "stakes",
                "trellis",
                "cage",
                "arbor",
                "fence"
            ],
            "type": "string"
        },
        "Temperature": {
            "properties": {
                "unit": {
                    "$ref": "#/definitions/TemperatureUnit"
                },
                "value": {
                    "type": "number"
                }
            },
            "required": [
                "unit",
                "value"
            ],
            "type": "object"
        },
        "TemperatureRange": {
            "properties": {
                "max": {
                    "$ref": "#/definitions/Temperature"
                },
                "min": {
                    "$ref": "#/definitions/Temperature"
                }
            },
            "required": [
                "max",
                "min"
            ],
            "type": "object"
        },
        "TemperatureUnit": {
            "enum": [
                "fahrenheit",
                "celsius"
            ],
            "type": "string"
        },
        "TimeDuration": {
            "properties": {
                "unit": {
                    "$ref": "#/definitions/TimeUnit"
                },
                "value": {
                    "type": "number"
                }
            },
            "required": [
                "unit",
                "value"
            ],
            "type": "object"
        },
        "TimeDurationRange": {
            "properties": {
                "max": {
                    "$ref": "#/definitions/TimeDuration"
                },
                "min": {
                    "$ref": "#/definitions/TimeDuration"
                }
            },
            "required": [
                "max",
                "min"
            ],
            "type": "object"
        },
        "TimeUnit": {
            "enum": [
                "days",
                "hours",
                "minutes",
                "seconds"
            ],
            "type": "string"
        },
        "ToxicityInfo": {
            "description": "Safety information for plants around humans and animals",
            "properties": {
                "humanToxicity": {
                    "properties": {
                        "level": {
                            "$ref": "#/definitions/ToxicityLevel"
                        },
                        "notes": {
                            "type": "string"
                        },
                        "symptoms": {
                            "items": {
                                "type": "string"
                            },
                            "type": "array"
                        },
                        "toxicParts": {
                            "items": {
                                "$ref": "#/definitions/PlantPart"
                            },
                            "type": "array"
                        }
                    },
                    "required": [
                        "level"
                    ],
                    "type": "object"
                },
                "livestockToxicity": {
                    "properties": {
                        "affectedAnimals": {
                            "items": {
                                "type": "string"
                            },
                            "type": "array"
                        },
                        "level": {
                            "$ref": "#/definitions/ToxicityLevel"
                        },
                        "toxicParts": {
                            "items": {
                                "$ref": "#/definitions/PlantPart"
                            },
                            "type": "array"
                        }
                    },
                    "type": "object"
                },
                "petToxicity": {
                    "properties": {
                        "cats": {
                            "$ref": "#/definitions/ToxicityLevel"
                        },
                        "dogs": {
                            "$ref": "#/definitions/ToxicityLevel"
                        },
                        "symptoms": {
                            "items": {
                                "type": "string"
                            },
                            "type": "array"
                        },
                        "toxicParts": {
                            "items": {
                                "$ref": "#/definitions/PlantPart"
                            },
                            "type": "array"
                        }
                    },
                    "type": "object"
                },
                "skinContact": {
                    "properties": {
                        "allergen": {
                            "type": "boolean"
                        },
                        "irritant": {
                            "type": "boolean"
                        },
                        "severity": {
                            "$ref": "#/definitions/ToxicityLevel"
                        }
                    },
                    "type": "object"
                }
            },
            "type": "object"
        },
        "ToxicityLevel": {
            "description": "Toxicity levels for safety planning around children, pets, and livestock",
            "enum": [
                "safe",
                "mildly-toxic",
                "moderately-toxic",
                "highly-toxic",
